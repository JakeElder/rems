{
  "kind": "collectionType",
  "collectionName": "properties",
  "info": {
    "singularName": "property",
    "pluralName": "properties",
    "displayName": "Property",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "title": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "images": {
      "type": "media",
      "multiple": true,
      "required": false,
      "allowedTypes": [
        "images"
      ]
    },
    "purchasePrice": {
      "type": "integer"
    },
    "bedrooms": {
      "type": "integer",
      "required": true
    },
    "bathrooms": {
      "type": "integer",
      "required": true
    },
    "livingArea": {
      "type": "integer",
      "required": true
    },
    "location": {
      "type": "customField",
      "customField": "plugin::google-map-picker.place"
    },
    "rentalPrice": {
      "type": "integer"
    },
    "availableToPurchase": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "availableToRent": {
      "type": "boolean",
      "default": false,
      "required": true
    },
    "yearBuilt": {
      "type": "integer"
    },
    "address": {
      "type": "text"
    },
    "indoor_features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::indoor-feature.indoor-feature",
      "inversedBy": "properties"
    },
    "lot_features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::lot-feature.lot-feature",
      "inversedBy": "properties"
    },
    "outdoor_features": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::outdoor-feature.outdoor-feature",
      "inversedBy": "properties"
    },
    "property_type": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::property-type.property-type",
      "inversedBy": "properties"
    },
    "view_types": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::view-type.view-type",
      "inversedBy": "properties"
    },
    "description": {
      "type": "text",
      "required": true
    },
    "nearest_bts_station": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::bts-station.bts-station"
    },
    "nearest_mrt_station": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::mrt-station.mrt-station"
    }
  }
}
